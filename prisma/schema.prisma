// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}
model employer {
  id Int @id @default(autoincrement())
  name String @default("")
  email String @default("")
}

model seekers {
  id Int @id @default(autoincrement())
  name String
  email String
}
